{
  "name": "Binary.com Charts (TradingView)",
  "version": "1.2.8",
  "manifest_version": 2,
  "description": "Tradingview charts implementation for Binary.com",
  "homepage_url": "https://www.binary.com",
  "icons": {
    "16": "v1.2.8/browser_app_assets/binary-16x16px.png",
    "128": "v1.2.8/browser_app_assets/binary-128x128px.png"
  },
  "background":{
    "scripts": ["chrome_background.js"]
  },
  "browser_action": {
    "default_icon": "v1.2.8/browser_app_assets/binary-16x16px.png",
    "default_title": "Tradingview charts implementation for Binary.com"
  },
  "permissions": [
    "cookies",
    "https://*.binary.com/*"
  ]
}